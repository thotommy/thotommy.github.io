{
  "name": "tho-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./scripts/set-env.ts",
    "start": "npm run config -- --environment=dev && ng serve",
    "build": "npm run config -- --environment=prod && ng build",
    "build-prod": "npm run lint && npm run config -- --environment=prod && ng build --prod",
    "deploy": "npm run config -- --environment=prod && ng build --prod && ngh --branch=master",
    "build-deploy": "ngh --branch=master --dry-run",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.12",
    "@angular/cdk": "^9.1.1",
    "@angular/common": "~10.0.12",
    "@angular/compiler": "~10.0.12",
    "@angular/core": "~11.0.5",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~10.0.12",
    "@angular/material": "^9.1.1",
    "@angular/platform-browser": "~10.0.12",
    "@angular/platform-browser-dynamic": "~10.0.12",
    "@angular/router": "~10.0.12",
    "@swimlane/ngx-charts": "^14.0.0",
    "core-js": "^2.6.11",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.6.2",
    "smoothscroll-polyfill": "^0.4.4",
    "subsink": "^1.0.2",
    "t-writer.js": "^1.0.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.4",
    "@angular/cli": "~10.0.7",
    "@angular/compiler-cli": "~10.0.12",
    "@angular/language-service": "~10.0.12",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.54",
    "angular-cli-ghpages": "^1.0.0-rc.1",
    "codelyzer": "^5.1.2",
    "dotenv": "^8.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "prettier": "^2.1.0",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.9.7",
    "yargs": "^16.2.0"
  }
}
